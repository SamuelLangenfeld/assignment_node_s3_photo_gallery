<header class="page-header">
  <h1>{{user.username}}</h1>
</header>


{{#if (isEmpty users) }}
  <p class="text-danger">No Users :(</p>
{{else }}
<div class="container">
  <div class="row">
    {{#each users as |user| }}
      <div class="col-xs-4">
      <a href="/users/{{user.id}}"><p>Username: {{user.username}}</p></a>
       {{/each}}
      {{/if }}

      
      </div>
  </div>
  <div class="row">
   {{#each photos as |photo| }}
    <div class="col-xs-4">
     <img src="{{ photo.url }}" alt="Photo" class="img-responsive"></a>
     <a
          href="/photos/{{ photo.name }}?_method=delete"
          class="text-danger">
          &times;
        </a>
    </div>
    {{/each}}
  </div>
  </div>
